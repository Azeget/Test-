python
# –ü—Ä–∏–≤–µ—Ç—Å—Ç–≤–µ–Ω–Ω–æ–µ —Å–æ–æ–±—â–µ–Ω–∏–µ
welcome_message = "üñê–ü—Ä–∏–≤–µ—Çüñê\n‚åö–í–æ—Ç —Å–ø–∏—Å–æ–∫ –∞–≤—Ç–æ –¥–µ–π—Å—Ç–≤–∏–π‚åö\n"

# –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å—Ç–∞—Ç—É—Å–∞ –∞–≤—Ç–æ –¥–µ–π—Å—Ç–≤–∏—è
def check_status(status):
    if status:
        return "‚úÖ"  # –í–∫–ª—é—á–µ–Ω–æ
    else:
        return "‚ùå"  # –í—ã–∫–ª—é—á–µ–Ω–æ

# –°–ø–∏—Å–æ–∫ –∞–≤—Ç–æ –¥–µ–π—Å—Ç–≤–∏–π
auto_actions = [
    "1. –ê–≤—Ç–æ –¥–æ–±–∞–≤–ª–µ–Ω–∏–µ –≤ –¥—Ä—É–∑—å—è : {status}",
    "2. –ê–≤—Ç–æ —É–¥–∞–ª–µ–Ω–∏–µ —É–¥–∞–ª–µ–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π : {status}",
    "3. –ê–≤—Ç–æ —É–¥–∞–ª–µ–Ω–∏–µ –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π : {status}",
    "4. –ê–≤—Ç–æ –¥–æ–±–∞–≤–ª–µ–Ω–∏–µ –≤ –ß–° —É–¥–∞–ª–µ–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π : {status}",
    "5. –ê–≤—Ç–æ –¥–æ–±–∞–≤–ª–µ–Ω–∏–µ –≤ –ß–° –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π : {status}"
]

# –ö–æ–º–∞–Ω–¥–∞ –≤—ã–≤–æ–¥–∞ –∞–≤—Ç–æ –¥–µ–π—Å—Ç–≤–∏–π –∏ –∏—Ö —Å—Ç–∞—Ç—É—Å–æ–≤
def get_auto_actions(prefix):
    message = welcome_message
    for action in auto_actions:
        message += action.format(status=check_status(True)) + "\n"
    message += f"\n–í–∞—à –ø—Ä–∏—Ñ–µ–∫—Å : {prefix}\n"
    message += "–ï—Å–ª–∏ —Å—Ç–æ–∏—Ç {‚úÖ} —Ç–æ–≥–¥–∞ –∫–æ–º–∞–Ω–¥–∞ –≤–∫–ª—é—á–µ–Ω–∞ {‚ùå} –µ—Å–ª–∏ –∫–æ–º–∞–Ω–¥–∞ –≤—ã–∫–ª—é—á–µ–Ω–∞ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é –ø—Ä–∏—Ñ–µ–∫—Å \".–ª\""
    return message

# –ö–æ–º–∞–Ω–¥–∞ –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Å—Ç–∞—Ç—É—Å–∞ –∞–≤—Ç–æ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –≤ –¥—Ä—É–∑—å—è
def set_auto_add_friends(prefix, status):
    if status:
        return f"–í–∫–ª—é—á–µ–Ω–∏–µ {prefix}+–∞–¥—Ä"
    else:
        return f"–í—ã–∫–ª—é—á–µ–Ω–∏–µ {prefix}-–∞–¥—Ä"

# –ö–æ–º–∞–Ω–¥–∞ –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Å—Ç–∞—Ç—É—Å–∞ –∞–≤—Ç–æ —É–¥–∞–ª–µ–Ω–∏—è —É–¥–∞–ª–µ–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π
def set_auto_remove_deleted_users(prefix, status):
    if status:
        return f"–í–∫–ª—é—á–µ–Ω–∏–µ {prefix}+—É–¥—Å–æ–±–∞–∫"
    else:
        return f"–í—ã–∫–ª—é—á–µ–Ω–∏–µ {prefix}-—É–¥—Å–æ–±–∞–∫"

# –ö–æ–º–∞–Ω–¥–∞ –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Å—Ç–∞—Ç—É—Å–∞ –∞–≤—Ç–æ —É–¥–∞–ª–µ–Ω–∏—è –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π
def set_auto_remove_blocked_users(prefix, status):
    if status:
        return f"–í–∫–ª—é—á–µ–Ω–∏–µ {prefix}+—É–¥–∑–æ–±"
    else:
        return f"–í—ã–∫–ª—é—á–µ–Ω–∏–µ {prefix}-—É–¥–∑–æ–±"

# –ö–æ–º–∞–Ω–¥–∞ –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Å—Ç–∞—Ç—É—Å–∞ –∞–≤—Ç–æ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –≤ –ß–° —É–¥–∞–ª–µ–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π
def set_auto_add_to_blacklist_deleted_users(prefix, status):
    if status:
        return f"–í–∫–ª—é—á–µ–Ω–∏–µ {prefix}+—á—Å—Å–æ–±–∞–∫"
    else:
        return f"–í—ã–∫–ª—é—á–µ–Ω–∏–µ {prefix}-—á—Å—Å–æ–±–∞–∫"

# –ö–æ–º–∞–Ω–¥–∞ –¥–ª—è –∏–∑–º–µ–Ω–µ–Ω–∏—è —Å—Ç–∞—Ç—É—Å–∞ –∞–≤—Ç–æ –¥–æ–±–∞–≤–ª–µ–Ω–∏—è –≤ –ß–° –∑–∞–±–ª–æ–∫–∏—Ä–æ–≤–∞–Ω–Ω—ã—Ö –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª–µ–π
def set_auto_add_to_blacklist_blocked_users(prefix, status):
    if status:
        return f"–í–∫–ª—é—á–µ–Ω–∏–µ {prefix}+—á—Å–∑–æ–±"
    else:
        return f"–í—ã–∫–ª—é—á–µ–Ω–∏–µ {prefix}-—á—Å–∑–æ–±"

# –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±—Ä–∞–±–æ—Ç–∫–∏ –∫–æ–º–∞–Ω–¥ –ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è
def process_command(command, prefix=".–ª"):
    if command == f"{prefix} –∞–≤—Ç–æ–¥":
        return get_auto_actions(prefix)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ +–∞–¥—Ä":
        return set_auto_add_friends(prefix, True)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ -–∞–¥—Ä":
        return set_auto_add_friends(prefix, False)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ +—É–¥—Å–æ–±–∞–∫":
        return set_auto_remove_deleted_users(prefix, True)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ -—É–¥—Å–æ–±–∞–∫":
        return set_auto_remove_deleted_users(prefix, False)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ +—É–¥–∑–æ–±":
        return set_auto_remove_blocked_users(prefix, True)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ -—É–¥–∑–æ–±":
        return set_auto_remove_blocked_users(prefix, False)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ +—á—Å—Å–æ–±–∞–∫":
        return set_auto_add_to_blacklist_deleted_users(prefix, True)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ -—á—Å—Å–æ–±–∞–∫":
        return set_auto_add_to_blacklist_deleted_users(prefix, False)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ +—á—Å–∑–æ–±":
        return set_auto_add_to_blacklist_blocked_users(prefix, True)
    elif command == f"{prefix} –∞–≤—Ç–æ–¥ -—á—Å–∑–æ–±":
        return set_auto_add_to_blacklist_blocked_users(prefix, False)
    else:
        return "–ù–µ–¥–æ–ø—É—Å—Ç–∏–º–∞—è –∫–æ–º–∞–Ω–¥–∞"
